        <input id="btnAddStation" type="button" value="Add station" class="btn btn-primary">
        <br />
    </div>
    {{/isAdmin}}
    <table class="table">
        <thead>
            <tr>
                <th scope="col">ID</th>
                <th scope="col">Name</th>
                <th scope="col">Type</th>
                <th scope="col">Position</th>
                <th scope="col">Status</th>

            </tr>
        </thead>
        <tbody>
            {{#stations}}
            <tr>
                <td>{{id}}</td>
                <td>{{station_name}}</td>
                <td>{{station_type}}</td>
                <td>{{station_position}}</td>
                <td>{{station_status}}</td>
            </tr>
            {{/stations}}
        </tbody>

    </table>
    </div>
</body>

</html>